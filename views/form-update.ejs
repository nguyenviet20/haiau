<h1>Sửa</h1>
<form action="/update" method="post">

    <table border="1">
        <tr>
            <th>LABLE</th>
            <th>VALUE</th>
        </tr>
        <tr>
            <td>ID:</td>
            <td><input type="text" name="id" value="<%= custommertwo.id %>"></td>
        </tr>
        <tr>
            <td>Phone:</td>
            <td><input type="number" name="phone" value="<%= custommertwo.phone %>"></td>
        </tr>
        <tr>
            <td>Password:</td>
            <td><input type="text" name="password" value="<%= custommertwo.password %>"></td>
        </tr>
        <tr>
            <td>TypeRegister:</td>
            <td><input type="text" name="register" value="<%= custommertwo.register %>"></td>
        </tr>
        <tr>
            <td>Full name:</td>
            <td><input type="text" name="fname" value="<%= custommertwo.fname %>"></td>
        </tr>
        <tr>
            <td>Type User:</td>
            <td><input type="number" name="type" value="<%= custommertwo.type %>"></td>
        </tr>

    </table>


    <button type="submit">Sửa</button>
</form>